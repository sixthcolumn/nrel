<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://enterprise.netbeans.org/bpel/Epri2/mediator" xmlns:ns2="http://j2ee.netbeans.org/wsdl/Epri2/src/LogOutput" xmlns:ns3="http://j2ee.netbeans.org/wsdl/Epri2/src/LogInput" xmlns:ns4="http://ws.server.sixthc.com/" xmlns:ns5="Epri2App" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>Epri2App</name>
            <description>Represents the Service Assembly of Epri2App</description>
        </identification>
        <service-unit>
            <identification>
                <name>Epri2App-Epri2</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>Epri2.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>Epri2App-sun-file-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-file-binding.jar</artifacts-zip>
                <component-name>sun-file-binding</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>Epri2App-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns1:LogFileOutput"/>
                <provider endpoint-name="LogOutput_OutboundPort" service-name="ns2:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns1:LogFileInput"/>
                <provider endpoint-name="LogInput_OutboundPort" service-name="ns3:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="ExecuteMaintOrderCreatePort" service-name="ns4:ExecuteMaintOrderCreateService"/>
                <provider endpoint-name="MaintenanceOrders_PortRole_myRole" service-name="ns1:Client"/>
            </connection>
            <connection>
                <consumer endpoint-name="MaintenanceOrders_PortRole_partnerRole" service-name="ns1:Provider"/>
                <provider endpoint-name="casaPort1" service-name="ns5:Epri2AppService1"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
