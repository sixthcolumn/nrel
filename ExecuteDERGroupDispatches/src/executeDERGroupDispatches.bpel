<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="executeDERGroupDispatches"
    targetNamespace="http://enterprise.netbeans.org/bpel/ExecuteDERGroupDispatches/executeDERGroupDispatches"
    xmlns:tns="http://enterprise.netbeans.org/bpel/ExecuteDERGroupDispatches/executeDERGroupDispatches"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:ns0="http://www.epri.com/2016/DERGroupDispatchesMessage" xmlns:ns1="http://www.iec.ch/TC57/2011/schema/message" xmlns:sxnmp="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/NMProperty">
    <import namespace="http://enterprise.netbeans.org/bpel/ExecuteDERGroupDispatchesWrapper" location="ExecuteDERGroupDispatchesWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://www.epri.com/2016/ExecuteDERGroupDispatches" location="file:/usr/local/git/nrel/61968/wsdl/ExecuteDERGroupDispatches.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/GetVendorURL/src/getVendor" location="getVendor.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="VendorLink" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetVendorURL/src/getVendor" partnerLinkType="tns:getVendor" partnerRole="getVendorPortTypeRole"/>
        <partnerLink name="RemoteLink" xmlns:tns="http://enterprise.netbeans.org/bpel/ExecuteDERGroupDispatchesWrapper" partnerLinkType="tns:DERGroupDispatches_PortLinkType" partnerRole="DERGroupDispatches_PortRole"/>
        <partnerLink name="ServiceInLink" xmlns:tns="http://enterprise.netbeans.org/bpel/ExecuteDERGroupDispatchesWrapper" partnerLinkType="tns:DERGroupDispatches_PortLinkType" myRole="DERGroupDispatches_PortRole"/>
    </partnerLinks>
    <sequence>
        <scope name="Scope1">
            <variables>
                <variable name="CreateDERGroupDispatchesOut" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" messageType="tns:ResponseMessage"/>
                <variable name="RemoteLinkOut" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" messageType="tns:ResponseMessage"/>
                <variable name="RemoteLinkIn" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" messageType="tns:CreateDERGroupDispatchesRequestMessage"/>
                <variable name="FaultOut" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" messageType="tns:ResponseMessage"/>
                <variable name="GetVendorOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetVendorURL/src/getVendor" messageType="tns:getVendorOperationResponse"/>
                <variable name="GetVendorOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetVendorURL/src/getVendor" messageType="tns:getVendorOperationRequest"/>
                <variable name="CreateDERGroupDispatchesIn" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" messageType="tns:CreateDERGroupDispatchesRequestMessage"/>
            </variables>
            <faultHandlers>
                <catch faultName="sxeh:systemFault" faultVariable="faultName" faultMessageType="sxeh:faultMessage">
                    <sequence name="Sequence2">
                        <assign name="Assign2">
                            <copy>
                                <from variable="faultName" part="fault"/>
                                <to>$FaultOut.ResponseMessage/ns0:Reply/ns1:Error/ns1:reason</to>
                            </copy>
                            <copy>
                                <from>'6005'</from>
                                <to>$FaultOut.ResponseMessage/ns0:Reply/ns1:Error/ns1:code</to>
                            </copy>
                            <copy>
                                <from>'FATAL'</from>
                                <to>$FaultOut.ResponseMessage/ns0:Reply/ns1:Error/ns1:level</to>
                            </copy>
                        </assign>
                        <reply name="ReplyFault" partnerLink="ServiceInLink" operation="CreateDERGroupDispatches" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" portType="tns:DERGroupDispatches_Port" variable="FaultOut"/>
                    </sequence>
                </catch>
            </faultHandlers>
            <sequence name="Sequence1">
                <receive name="Receive1" createInstance="yes" partnerLink="ServiceInLink" operation="CreateDERGroupDispatches" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" portType="tns:DERGroupDispatches_Port" variable="CreateDERGroupDispatchesIn"/>
                <assign name="Assign1">
                    <copy>
                        <from>$CreateDERGroupDispatchesIn.CreateDERGroupDispatchesRequestMessage/ns0:Header/ns1:User/ns1:Organization</from>
                        <to variable="GetVendorOperationIn" part="vendorName"/>
                    </copy>
                    <copy>
                        <from>'CIM_DER (createDERGroupDispatch)'</from>
                        <to variable="GetVendorOperationIn" part="opName"/>
                    </copy>
                </assign>
                <invoke name="Invoke1" partnerLink="VendorLink" operation="getVendorOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetVendorURL/src/getVendor" portType="tns:getVendorPortType" inputVariable="GetVendorOperationIn" outputVariable="GetVendorOperationOut"/>
                <assign name="Assign3">
                    <copy>
                        <from variable="GetVendorOperationOut" part="url"/>
                        <to variable="RemoteLinkIn" sxnmp:nmProperty="org.glassfish.openesb.outbound.address.url"/>
                    </copy>
                    <copy>
                        <from>$CreateDERGroupDispatchesIn.CreateDERGroupDispatchesRequestMessage/ns0:Header</from>
                        <to>$RemoteLinkIn.CreateDERGroupDispatchesRequestMessage/ns0:Header</to>
                    </copy>
                    <copy>
                        <from>$CreateDERGroupDispatchesIn.CreateDERGroupDispatchesRequestMessage/ns0:Request</from>
                        <to>$RemoteLinkIn.CreateDERGroupDispatchesRequestMessage/ns0:Request</to>
                    </copy>
                    <copy>
                        <from>$CreateDERGroupDispatchesIn.CreateDERGroupDispatchesRequestMessage/ns0:Payload</from>
                        <to>$RemoteLinkIn.CreateDERGroupDispatchesRequestMessage/ns0:Payload</to>
                    </copy>
                </assign>
                <invoke name="Invoke2" partnerLink="RemoteLink" operation="CreateDERGroupDispatches" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" portType="tns:DERGroupDispatches_Port" inputVariable="RemoteLinkIn" outputVariable="RemoteLinkOut"/>
                <assign name="Assign4">
                    <copy>
                        <from>$RemoteLinkOut.ResponseMessage/ns0:Header</from>
                        <to>$CreateDERGroupDispatchesOut.ResponseMessage/ns0:Header</to>
                    </copy>
                    <copy>
                        <from>$RemoteLinkOut.ResponseMessage/ns0:Reply</from>
                        <to>$CreateDERGroupDispatchesOut.ResponseMessage/ns0:Reply</to>
                    </copy>
                    <copy>
                        <from>$RemoteLinkOut.ResponseMessage/ns0:Payload</from>
                        <to>$CreateDERGroupDispatchesOut.ResponseMessage/ns0:Payload</to>
                    </copy>
                </assign>
                <reply name="Reply1" partnerLink="ServiceInLink" operation="CreateDERGroupDispatches" xmlns:tns="http://www.epri.com/2016/ExecuteDERGroupDispatches" portType="tns:DERGroupDispatches_Port" variable="CreateDERGroupDispatchesOut"/>
            </sequence>
        </scope>
    </sequence>
</process>
