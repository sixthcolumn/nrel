<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://enterprise.netbeans.org/bpel/CreateMaintOrder/createMaintOrder" xmlns:ns2="http://j2ee.netbeans.org/wsdl/CreateMaintOrder/src/LogRequest" xmlns:ns3="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" xmlns:ns4="http://j2ee.netbeans.org/wsdl/CreateMaintOrder/src/LogReply" xmlns:ns5="CreateMaintOrderApp" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>CreateMaintOrderApp</name>
            <description>Represents the Service Assembly of CreateMaintOrderApp</description>
        </identification>
        <service-unit>
            <identification>
                <name>CreateMaintOrderApp-CreateMaintOrder</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>CreateMaintOrder.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>CreateMaintOrderApp-sun-file-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-file-binding.jar</artifacts-zip>
                <component-name>sun-file-binding</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>CreateMaintOrderApp-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns1:RequestLog"/>
                <provider endpoint-name="LogRequest_OutboundPort" service-name="ns2:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="MaintenanceOrders_Port" service-name="ns3:ExecuteMaintenanceOrders"/>
                <provider endpoint-name="MaintenanceOrders_PortRole_myRole" service-name="ns1:Service"/>
            </connection>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns1:R1Log"/>
                <provider endpoint-name="LogReply_OutboundPort" service-name="ns4:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns1:R2Log"/>
                <provider endpoint-name="casaPort3" service-name="ns5:CreateMaintOrderAppService3"/>
            </connection>
            <connection>
                <consumer endpoint-name="MaintenanceOrders_PortRole_partnerRole" service-name="ns1:R1"/>
                <provider endpoint-name="casaPort1" service-name="ns5:CreateMaintOrderAppService1"/>
            </connection>
            <connection>
                <consumer endpoint-name="MaintenanceOrders_PortRole_partnerRole" service-name="ns1:R2"/>
                <provider endpoint-name="casaPort2" service-name="ns5:CreateMaintOrderAppService2"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
