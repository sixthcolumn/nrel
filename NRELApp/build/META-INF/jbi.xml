<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="http://enterprise.netbeans.org/bpel/MaintOrder/maintOrder" xmlns:ns10="NRELApp" xmlns:ns2="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogOutboundMessage" xmlns:ns3="http://enterprise.netbeans.org/bpel/GetMaintOrder/getMaintOrder" xmlns:ns4="http://j2ee.netbeans.org/wsdl/GetMaintOrder/src/LogRequest" xmlns:ns5="http://j2ee.netbeans.org/wsdl/GetMaintOrder/src/LogResponse" xmlns:ns6="http://j2ee.netbeans.org/wsdl/GetMaintOrder/src/Log3" xmlns:ns7="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogReplies" xmlns:ns8="http://iec.ch/TC57/2015/GetMaintenanceOrders" xmlns:ns9="http://ws.server.sixthc.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>NRELApp</name>
            <description>Represents the Service Assembly of NRELApp</description>
        </identification>
        <service-unit>
            <identification>
                <name>NRELApp-GetMaintOrder</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>GetMaintOrder.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>NRELApp-CreateMaintOrder</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>CreateMaintOrder.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>NRELApp-sun-file-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-file-binding.jar</artifacts-zip>
                <component-name>sun-file-binding</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>NRELApp-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns1:Log1"/>
                <provider endpoint-name="LogOutboundMessage_OutboundPort" service-name="ns2:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns3:LogRequest"/>
                <provider endpoint-name="LogRequest_OutboundPort" service-name="ns4:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns3:LogResponse"/>
                <provider endpoint-name="LogResponse_OutboundPort" service-name="ns5:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns3:Log3"/>
                <provider endpoint-name="Log3_OutboundPort" service-name="ns6:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="FileOutboundPortTypeRole_partnerRole" service-name="ns1:Log2"/>
                <provider endpoint-name="LogReplies_OutboundPort" service-name="ns7:FileOutboundService"/>
            </connection>
            <connection>
                <consumer endpoint-name="GetMaintenanceOrders_Port" service-name="ns8:GetMaintenanceOrders"/>
                <provider endpoint-name="GetMaintenanceOrders_PortRole_myRole" service-name="ns3:Service"/>
            </connection>
            <connection>
                <consumer endpoint-name="ExecuteMaintOrderCreatePort" service-name="ns9:ExecuteMaintOrderCreateService"/>
                <provider endpoint-name="MaintenanceOrders_PortRole_myRole" service-name="ns1:Create"/>
            </connection>
            <connection>
                <consumer endpoint-name="MaintenanceOrders_PortRole_partnerRole" service-name="ns1:R1"/>
                <provider endpoint-name="casaPort1" service-name="ns10:NRELAppService1"/>
            </connection>
            <connection>
                <consumer endpoint-name="MaintenanceOrders_PortRole_partnerRole" service-name="ns1:R2"/>
                <provider endpoint-name="casaPort2" service-name="ns10:NRELAppService2"/>
            </connection>
            <connection>
                <consumer endpoint-name="GetMaintenanceOrders_PortRole_partnerRole" service-name="ns3:R1"/>
                <provider endpoint-name="casaPort3" service-name="ns10:NRELAppService3"/>
            </connection>
            <connection>
                <consumer endpoint-name="GetMaintenanceOrders_PortRole_partnerRole" service-name="ns3:R2"/>
                <provider endpoint-name="casaPort4" service-name="ns10:NRELAppService4"/>
            </connection>
        </connections>
    </service-assembly>
</jbi>
