        <partnerLink name="R2" xmlns:tns="http://enterprise.netbeans.org/bpel/ExecuteMaintenanceOrdersWrapper" partnerLinkType="tns:R2_PortLinkType" partnerRole="MaintenanceOrders_PortRole"/>
