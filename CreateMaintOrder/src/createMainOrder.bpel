        <partnerLink name="R2Log" xmlns:tns="http://j2ee.netbeans.org/wsdl/CreateMainOrder/src/ReplyLog" partnerLinkType="tns:ReplyLog" partnerRole="FileOutboundPortTypeRole"/>
        <partnerLink name="Service" xmlns:tns="http://enterprise.netbeans.org/bpel/ExecuteMaintenanceOrdersWrapper" partnerLinkType="tns:Service_PortLinkType" myRole="MaintenanceOrders_PortRole"/>
