<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="maintOrder"
    targetNamespace="http://enterprise.netbeans.org/bpel/MaintOrder/maintOrder"
    xmlns:tns="http://enterprise.netbeans.org/bpel/MaintOrder/maintOrder"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" xmlns:ns2="http://iec.ch/TC57/2015/MaintenanceOrdersMessage">
    <import namespace="http://enterprise.netbeans.org/bpel/MaintOrderServiceCreateWrapper" location="MaintOrderServiceCreateWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://ws.server.sixthc.com/" location="http://localhost:8080/epriConnect/MaintOrderServiceCreate?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" location="http://localhost:8080/epriConnect/MaintOrderServiceCreate?wsdl=MaintenanceOrders_Port.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/MaintOrderServiceCreateERemote1" location="MaintOrderServiceCreateRemote1.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/MaintOrderServiceCreateRemote2" location="MaintOrderServiceCreateRemote2.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogOutboundMessage" location="LogOutboundMessage.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogReplies" location="LogReplyMessages.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="R1" xmlns:tns="http://enterprise.netbeans.org/bpel/MaintOrderServiceCreateERemote1" partnerLinkType="tns:MaintenanceOrders_PortLinkType" partnerRole="MaintenanceOrders_PortRole"/>
        <partnerLink name="R2" xmlns:tns="http://enterprise.netbeans.org/bpel/MaintOrderServiceCreateRemote2" partnerLinkType="tns:MaintenanceOrders_PortLinkType" partnerRole="MaintenanceOrders_PortRole"/>
        <partnerLink name="Log1" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogOutboundMessage" partnerLinkType="tns:LogOutboundMessage" partnerRole="FileOutboundPortTypeRole"/>
        <partnerLink name="Log2" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogReplies" partnerLinkType="tns:LogReplies" partnerRole="FileOutboundPortTypeRole"/>
        <partnerLink name="Create" xmlns:tns="http://enterprise.netbeans.org/bpel/MaintOrderServiceCreateWrapper" partnerLinkType="tns:MaintenanceOrders_PortLinkType" myRole="MaintenanceOrders_PortRole"/>
    </partnerLinks>
    <variables>
        <variable name="R2Log" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogReplies" messageType="tns:WriteInputMessage"/>
        <variable name="R1Log" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogReplies" messageType="tns:WriteInputMessage"/>
        <variable name="CreateLog" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogOutboundMessage" messageType="tns:WriteInputMessage"/>
        <variable name="CreateIn" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="ns1:CreateMaintenanceOrders"/>
        <variable name="CreateResponse" messageType="ns1:CreateMaintenanceOrdersResponse"/>
        <variable name="R1In" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="ns1:CreateMaintenanceOrders"/>
        <variable name="R1Response" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="ns1:CreateMaintenanceOrdersResponse"/>
        <variable name="R2In" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="ns1:CreateMaintenanceOrders"/>
        <variable name="R2Response" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="ns1:CreateMaintenanceOrdersResponse"/>
    </variables>
    <sequence>
        <receive name="RecieveClientMessage" createInstance="yes" partnerLink="Create" operation="CreateMaintenanceOrders" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" portType="ns1:MaintenanceOrders_Port" variable="CreateIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="CreateIn"/>
                <to variable="R1In"/>
            </copy>
            <copy>
                <from variable="CreateIn"/>
                <to variable="R2In"/>
            </copy>
            <copy>
                <from>$CreateIn.parameters/ns2:Header</from>
                <to>$CreateLog.part1/ns2:Header</to>
            </copy>
            <copy>
                <from>$CreateIn.parameters/ns2:Request</from>
                <to>$CreateLog.part1/ns2:Request</to>
            </copy>
            <copy>
                <from>$CreateIn.parameters/ns2:Payload</from>
                <to>$CreateLog.part1/ns2:Payload</to>
            </copy>
        </assign>
        <invoke name="InvokeR1" partnerLink="R1" operation="CreateMaintenanceOrders" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" portType="ns1:MaintenanceOrders_Port" inputVariable="R1In" outputVariable="R1Response"/>
        <invoke name="InvokeR2" partnerLink="R2" operation="CreateMaintenanceOrders" xmlns:ns1="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" portType="ns1:MaintenanceOrders_Port" inputVariable="R2In" outputVariable="R2Response"/>
        <assign name="Assign2">
            <copy>
                <from variable="R1Response"/>
                <to variable="CreateResponse"/>
            </copy>
            <copy>
                <from>$R2Response.parameters/ns2:Header</from>
                <to>$R2Log.part1/ns2:Header</to>
            </copy>
            <copy>
                <from>$R2Response.parameters/ns2:Reply</from>
                <to>$R2Log.part1/ns2:Reply</to>
            </copy>
            <copy>
                <from>$R2Response.parameters/ns2:Payload</from>
                <to>$R2Log.part1/ns2:Payload</to>
            </copy>
            <copy>
                <from>$R1Response.parameters/ns2:Header</from>
                <to>$R1Log.part1/ns2:Header</to>
            </copy>
            <copy>
                <from>$R1Response.parameters/ns2:Reply</from>
                <to>$R1Log.part1/ns2:Reply</to>
            </copy>
            <copy>
                <from>$R1Response.parameters/ns2:Payload</from>
                <to>$R1Log.part1/ns2:Payload</to>
            </copy>
        </assign>
        <invoke name="R1LogMsg" partnerLink="Log1" operation="write" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogOutboundMessage" portType="tns:FileOutboundPortType" inputVariable="CreateLog"/>
        <invoke name="R1LogReply" partnerLink="Log2" operation="write" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogReplies" portType="tns:FileOutboundPortType" inputVariable="R1Log"/>
        <invoke name="R2LogReply" partnerLink="Log2" operation="write" xmlns:tns="http://j2ee.netbeans.org/wsdl/MaintOrder/src/LogReplies" portType="tns:FileOutboundPortType" inputVariable="R2Log"/>
        <reply name="Recieve" partnerLink="Create" operation="CreateMaintenanceOrders" portType="ns1:MaintenanceOrders_Port" variable="CreateResponse"/>
    </sequence>
</process>
