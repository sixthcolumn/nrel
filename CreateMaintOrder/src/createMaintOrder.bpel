<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="createMaintOrder"
    targetNamespace="http://enterprise.netbeans.org/bpel/CreateMaintOrder/createMaintOrder"
    xmlns:tns="http://enterprise.netbeans.org/bpel/CreateMaintOrder/createMaintOrder"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxnmp="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/NMProperty" xmlns:ns0="http://iec.ch/TC57/2015/MaintenanceOrdersMessage" xmlns:ns1="http://iec.ch/TC57/2011/schema/message" xmlns:ns2="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://enterprise.netbeans.org/bpel/ExecuteMaintenanceOrdersWrapper" location="ExecuteMaintenanceOrdersWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" location="file:/usr/local/git/nrel/61968/wsdl/CimCreate/ExecuteMaintenanceOrders.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/CreateMaintOrder/src/LogRequest" location="LogRequest.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/CreateMaintOrder/src/LogReply" location="LogReply.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/GetDyn/src/jdbc" location="jdbc.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/GetMaintenanceOrdersWrapper" location="GetMaintenanceOrdersWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://iec.ch/TC57/2015/GetMaintenanceOrders" location="GetDyn/GetMaintenanceOrders.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="JDBC" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetDyn/src/jdbc" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="R1" xmlns:tns="http://enterprise.netbeans.org/bpel/ExecuteMaintenanceOrdersWrapper" partnerLinkType="tns:R1_PortLinkType" partnerRole="MaintenanceOrders_PortRole"/>
        <partnerLink name="R2" xmlns:tns="http://enterprise.netbeans.org/bpel/GetMaintenanceOrdersWrapper" partnerLinkType="tns:GetMaintenanceOrders_PortLinkType" partnerRole="GetMaintenanceOrders_PortRole"/>
        <partnerLink name="Service" xmlns:tns="http://enterprise.netbeans.org/bpel/ExecuteMaintenanceOrdersWrapper" partnerLinkType="tns:Service_PortLinkType" myRole="MaintenanceOrders_PortRole"/>
    </partnerLinks>
    <variables>
        <variable name="VendorIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetDyn/src/jdbc" messageType="tns:inputVendorUrl"/>
        <variable name="FaultOut" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="tns:FaultMessage"/>
        <variable name="R2Out" xmlns:tns="http://iec.ch/TC57/2015/GetMaintenanceOrders" messageType="tns:GetMaintenanceOrdersResponseMessage"/>
        <variable name="R2In" xmlns:tns="http://iec.ch/TC57/2015/GetMaintenanceOrders" messageType="tns:GetMaintenanceOrdersRequestMessage"/>
        <variable name="JDBCOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetDyn/src/jdbc" messageType="tns:outputMsg"/>
        <variable name="ServiceReplyOut" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="tns:ResponseMessage"/>
        <variable name="R1Out" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="tns:ResponseMessage"/>
        <variable name="R1In" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="tns:CreateMaintenanceOrdersRequestMessage"/>
        <variable name="ServiceIn" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" messageType="tns:CreateMaintenanceOrdersRequestMessage"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="Service" operation="CreateMaintenanceOrders" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" portType="tns:MaintenanceOrders_Port" variable="ServiceIn"/>
        <assign name="aJDBC">
            <copy>
                <from>$ServiceIn.CreateMaintenanceOrdersRequestMessage/ns0:Header/ns1:User/ns1:Organization</from>
                <to>$VendorIn.part1/ns2:vendor</to>
            </copy>
            <copy>
                <from>'CIM_DER(createDERGroup)'</from>
                <to>$VendorIn.part1/ns2:message name</to>
            </copy>
            <copy>
                <from>'standalone'</from>
                <to>$VendorIn.part1/ns2:test seq type</to>
            </copy>
            <copy>
                <from>'CIM DER'</from>
                <to>$VendorIn.part1/ns2:package name</to>
            </copy>
            <copy>
                <from>'CIM_DER(createDERGroup)'</from>
                <to>$VendorIn.part1/ns2:message_name</to>
            </copy>
            <copy>
                <from>'standalone'</from>
                <to>$VendorIn.part1/ns2:test_seq_type</to>
            </copy>
            <copy>
                <from>'CIM DER'</from>
                <to>$VendorIn.part1/ns2:package_name</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="JDBC" operation="get_vendor_url" xmlns:tns="http://j2ee.netbeans.org/wsdl/GetDyn/src/jdbc" portType="tns:jdbcPortType" inputVariable="VendorIn" outputVariable="JDBCOut"/>
        <if name="NotNull">
            <condition>$JDBCOut.part != ''</condition>
            <sequence name="Sequence1">
                <assign name="Assign1">
                    <copy>
                        <from>concat($JDBCOut.part, '/MaintOrderServiceCreate')</from>
                        <to variable="R1In" sxnmp:nmProperty="org.glassfish.openesb.outbound.address.url"/>
                    </copy>
                    <copy>
                        <from variable="ServiceIn" part="CreateMaintenanceOrdersRequestMessage"/>
                        <to variable="R1In" part="CreateMaintenanceOrdersRequestMessage"/>
                    </copy>
                    <copy>
                        <from variable="ServiceIn" part="CreateMaintenanceOrdersRequestMessage"/>
                        <to variable="R2In" part="GetMaintenanceOrdersRequestMessage"/>
                    </copy>
                </assign>
                <invoke name="InvokeR1" partnerLink="R1" operation="CreateMaintenanceOrders" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" portType="tns:MaintenanceOrders_Port" inputVariable="R1In" outputVariable="R1Out"/>
                <invoke name="InvokeR2" partnerLink="R2" operation="GetMaintenanceOrders" xmlns:tns="http://iec.ch/TC57/2015/GetMaintenanceOrders" portType="tns:GetMaintenanceOrders_Port" inputVariable="R2In" outputVariable="R2Out"/>
                <assign name="Assign2">
                    <copy>
                            <from variable="R1Out"/>
                                <to variable="ServiceReplyOut"/>
                        </copy>
                </assign>
                <reply name="ServiceReply" partnerLink="Service" operation="CreateMaintenanceOrders" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" portType="tns:MaintenanceOrders_Port" variable="ServiceReplyOut"/>
            </sequence>
            <else>
                <sequence name="Sequence2">
                    <assign name="Assign3">
                        <copy>
                            <from>'6001'</from>
                            <to>$FaultOut.FaultMessage/ns0:Reply/ns1:Error/ns1:code</to>
                        </copy>
                        <copy>
                            <from>'FAILED'</from>
                            <to>$FaultOut.FaultMessage/ns0:Reply/ns1:Result</to>
                        </copy>
                        <copy>
                            <from>'FATAL'</from>
                            <to>$FaultOut.FaultMessage/ns0:Reply/ns1:Error/ns1:level</to>
                        </copy>
                        <copy>
                            <from>'Lookup of Header.User.Organization failed'</from>
                            <to>$FaultOut.FaultMessage/ns0:Reply/ns1:Error/ns1:reason</to>
                        </copy>
                    </assign>
                    <reply name="Reply1" partnerLink="Service" operation="CreateMaintenanceOrders" xmlns:tns="http://iec.ch/TC57/2015/ExecuteMaintenanceOrders" portType="tns:MaintenanceOrders_Port" faultName="tns:CreateMaintenanceOrdersFault" variable="FaultOut"/>
                </sequence>
            </else>
        </if>
    </sequence>
</process>
